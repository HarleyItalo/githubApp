    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Back Page" Clicked="Handle_Clicked">
        </ToolbarItem> 
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <WebView Source="{Binding URL}"  x:Name="WebViewData" />
    </ContentPage.Content>
